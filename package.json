{
	"name": "@haut/vtf",
	"version": "0.1.0",
	"main": "dist/index.js",
	"browser": "dist/index.browser.js",
	"license": "MIT",
	"dependencies": {
		"@saschazar/wasm-webp": "^1.3.1",
		"chalk": "^4.0.0",
		"decode-dxt": "^1.0.1",
		"dxt-js": "^0.0.3",
		"file-type": "^14.5.0",
		"jimp": "^0.12.1",
		"tga": "^1.0.3"
	},
	"devDependencies": {
		"@babel/core": "^7.10.1",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-export-default-from": "^7.10.1",
		"@babel/plugin-proposal-logical-assignment-operators": "^7.10.1",
		"@babel/plugin-proposal-numeric-separator": "^7.10.1",
		"@babel/plugin-proposal-partial-application": "^7.10.1",
		"@babel/plugin-proposal-pipeline-operator": "^7.10.1",
		"@babel/plugin-proposal-private-property-in-object": "^7.10.1",
		"@babel/plugin-proposal-throw-expressions": "^7.10.1",
		"@babel/plugin-syntax-module-attributes": "^7.10.2",
		"@babel/plugin-transform-runtime": "^7.9.6",
		"@babel/preset-env": "^7.9.6",
		"@pumpn/eslint-config": "^4.0.0",
		"eslint": "^7.1.0",
		"jsdoc": "^3.6.4",
		"jsdoc-babel": "^0.5.0",
		"parcel": "^2.0.0-nightly.275"
	},
	"scripts": {
		"build": "parcel build src/index.js",
		"rundist": "node dist/index.js --no-deprecation",
		"start": "parcel build --target main src/index.js --no-minify && yarn rundist",
		"docs": "rm -rf ./docs/ && jsdoc -c ./.jsdocrc -r --verbose"
	},
	"engines": {
		"node": ">=14.x"
	},
	"targets": {
		"main": {
			"context": "node",
			"outputFormat": "commonjs",
			"sourceMap": {
				"inline": true,
				"inlineSources": true
			},
			"engines": {
				"node": ">=14.x"
			},
			"isLibrary": true
		},
		"browser": {
			"context": "browser",
			"outputFormat": "global",
			"includeNodeModules": true,
			"engines": {
				"browsers": [
					"> 20%",
					"not dead"
				]
			},
			"isLibrary": true
		}
	}
}
